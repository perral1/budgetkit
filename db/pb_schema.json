    "indexes": [],
    "system": false,
    "viewQuery": "SELECT\n  t.id,\n  t.category,\n  t.month,\n  t.activity\nFROM (\n  SELECT\n    (c.id || '|' || strftime('%Y-%m', e.date)) AS id,\n    c.name AS category,\n    strftime('%Y-%m', e.date) AS month,\n    SUM(e.qty) AS activity\n  FROM entries e\n  JOIN categories c ON c.id = e.category\n  WHERE c.kind IN ('income', 'expense')\n  GROUP BY\n    strftime('%Y-%m', e.date),\n    c.name\n) t;\n"
  }
